{
  "name": "webio-jupyterlab-provider",
  "version": "0.1.0",
  "description": "A JupyterLab extension.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/github_username/webio-jupyterlab-provider",
  "bugs": {
    "url": "https://github.com/github_username/webio-jupyterlab-provider/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "",
    "email": ""
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}"
  ],
  "main": "dist/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/github_username/webio-jupyterlab-provider.git"
  },
  "scripts": {
    "pre-build": "tsc -p . && webpack ./lib/index.js",
    "build": "yarn pre-build && yarn build:lib && yarn build:labextension:dev",
    "build:prod": "yarn run clean && yarn run build:lib && yarn run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "clean": "yarn run clean:lib",
    "clean:lib": "rimraf lib dist tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf webio_jupyterlab_provider/labextension",
    "clean:all": "yarn run clean:lib && yarn run clean:labextension",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "install:extension": "yarn run build",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w",
    "watch:labextension": "jupyter labextension watch .",
    "publish-pypi": "yarn build && python3 -m build && python3 -m twine upload dist/*.whl dist/*.gz "
  },
  "dependencies": {
    "@jupyterlab/application": "^3",
    "@jupyterlab/docregistry": "^3",
    "@jupyterlab/notebook": "^3",
    "@jupyterlab/rendermime": "^3",
    "@jupyterlab/services": "^6",
    "@lumino/disposable": "^1",
    "@lumino/widgets": "^1",
    "@webio/webio": "portal:../packages/webio",
    "debug": "^4.3.2"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.1.0",
    "@types/debug": "^4.1.7",
    "@types/react": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "typescript": "~4.1.3",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "webio_jupyterlab_provider/labextension"
  },
  "jupyter-releaser": {
    "hooks": {
      "before-build-npm": [
        "python -m pip install jupyterlab~=3.1",
        "yarn"
      ]
    }
  }
}
