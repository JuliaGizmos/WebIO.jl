{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "composite": true,
    "declaration": true,
    "esModuleInterop": true,
    "incremental": true,
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "preserveWatchOutput": true,
    "resolveJsonModule": true,
    "outDir": "./lib",
    "rootDir": "./webio-jupyter-labextension",
    "strict": true,
    "strictNullChecks": true,
    "target": "es2017",
    "types": [],
    "importsNotUsedAsValues": "error",
    "skipLibCheck": true
  },
  "include": ["webio-jupyter-labextension/*"]
}
